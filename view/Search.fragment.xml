<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog title="Advanced Filter" confirm="onHandleFilterConfirm">
		<customTabs>
			<ViewSettingsCustomTab text="Data range">
				<content>
					<DatePicker placeholder="Begin Date" value="{view>/begin_date}" valueFormat="yyyyMMdd" minDate="{view>/min_date }"/>
					<DatePicker placeholder="End Date" value="{view>/end_date}" valueFormat="yyyyMMdd"/>
				</content>
			</ViewSettingsCustomTab>
		</customTabs>
	</ViewSettingsDialog>
</core:FragmentDefinition>